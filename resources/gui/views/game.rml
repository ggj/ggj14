<rml>
	<head>
		<title>game</title>
		<link type="text/template" href="gameplay.rml" />
	</head>
	<body id="game_window" onload="load">
		<game id="game">
			<div id="hostages_div">
				<icon />
				Hostages: <span id="score" />
			</div>
			<div id="time_div">
				<icon />
				Time: <span id="hiscore" />
			</div>
			<div id="lifes_div">
				<icon />
				Lives: <span id="lifes" /> x 3
			</div>
		</game>
	</body>
</rml>
